{
  "version": 2,
  "framework": "create-react-app",
  "functions": {
    "api/app.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/app.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}